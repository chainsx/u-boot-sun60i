		gsensor_int1 = <&pio PL 5 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>;
		gsensor-supply = <&reg_dldo4>;
		gsensor_vcc_io_val = <3300>;
	};
	lightsensor {
		compatible = "allwinner,stk342x";
		reg = <0x58>;
		device_type = "lightsensor";
		status = "okay";
		ls_twi_id = <0x1>;
