			};
			reg_dcdc8: dcdc8 {
				regulator-name = "axp8191-dcdc8";
			};
			reg_dcdc9: dcdc9 {
				regulator-name = "axp8191-dcdc9";
			};
			reg_rtcldo: rtcldo {
				regulator-name = "axp8191-rtcldo";
			};
			reg_aldo1: aldo1 {
				regulator-name = "axp8191-aldo1";
			};
			reg_aldo2: aldo2 {
				regulator-name = "axp8191-aldo2";
			};
			reg_aldo3: aldo3 {
