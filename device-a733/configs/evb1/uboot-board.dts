			};
			reg_cldo1: cldo1 {
				regulator-name = "axp8191-cldo1";
			};
			reg_cldo2: cldo2 {
				regulator-name = "axp8191-cldo2";
			};
			reg_cldo3: cldo3 {
				regulator-name = "axp8191-cldo3";
			};
			reg_cldo4: cldo4 {
