			};
			reg_aldo4: aldo4 {
				regulator-name = "axp8191-aldo4";
			};
			reg_aldo5: aldo5 {
				regulator-name = "axp8191-aldo5";
			};
			reg_aldo6: aldo6 {
				regulator-name = "axp8191-aldo6";
			};
			reg_bldo1: bldo1 {
