	pinctrl-0 = <&s_twi0_pins_a>;
	pinctrl-1 = <&s_twi0_pins_b>;
	twi-supply = <&reg_aldo3>;
	status = "okay";
	pmu1: pmu@36 {
		compatible = "x-powers,axp8191";
		reg = <0x36>;
		status = "okay";

		regulator0: regulators@0 {
			reg_dcdc1: dcdc1 {
				regulator-name = "axp8191-dcdc1";
			};
			reg_dcdc2: dcdc2 {
